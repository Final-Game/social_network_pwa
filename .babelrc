{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@Components": "./src/Components",
          "@Definitions": "./src/Definitions",
          "@Interfaces": "./src/Interfaces",
          "@Pages": "./pages",
          "@Reducers": "./src/Redux/Reducers",
          "@Services": "./src/Services",
          "@Server": "./server",
          "@Static": "./public/static",
          "@Styled": "./src/Styled"
        }
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ]
  ]
}